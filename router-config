conf t

banner motd #Restricted Area! Lords of the ping are looking for you!#

no ip domain-lookup

interface fa0/1
  ip address 10.44.0.1 255.255.255.0
  no shutdown
  description To-SW2
  exit

interface fa0/0
  ip address 10.34.0.2 255.255.255.0
//  no shutdown
  description To-R3
  exit

router eigrp 666
  passive-interface default
  no passive-interface fa0/0
  no passive-interface fa0/1
  network 10.44.0.0 0.0.0.255
  network 10.34.0.0 0.0.0.255

end
wr


configure terminal
no router eigrp 666
router ospf 666
passive-interface default
no passive-interface fa0/0
no passive-interface fa0/1
network 10.34.0.0 0.0.0.255 area 0
network 10.44.0.0 0.0.0.255 area 0
